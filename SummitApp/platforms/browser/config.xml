<?xml version='1.0' encoding='utf-8'?>
<widget build="0.0.8" id="com.ruff-media.summit" version="1.0.8" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>SummitAgroMéxico</name>
    <description>
        Summit Agro México - Aplicación informativa
    </description>
    <author email="contacto@ruff-media.com" href="http://cordova.io">
        RuffMedia
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="Fullscreen" value="true" />
    <preference name="WebViewBounce" value="true" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarBackgroundColor" value="#3B8D25" />
    <preference name="StatusBarStyle" value="lightcontent" />
</widget>
